{ "manifest_version"        : 2
, "minimum_chrome_version"  : "28.0"

, "default_locale"          : "en_US"
, "name"                    : "__MSG_name_long__"
, "short_name"              : "__MSG_name_shrt__"
, "description"             : "__MSG_name_desc__"

, "version"                 : "2.0.9.1"
, "homepage_url"            : "https://github.com/eladkarako/Chrome-Extension-Boostaler/"
, "author"                  : "Elad Karako"

, "offline_enabled"         : true

, "permissions"             : ["*://*/*"
                              ,"webRequest"
                              ,"webRequestBlocking"
                              ]
, "background"              : { "scripts":    [ 
                                                "resources/background_response_manipulation.js"
                                              ]
                              , "persistent": true 
                              }
,"web_accessible_resources" : [ "resources/*.*"]
,"icons"                    : { "512": "resources/icon.png" }
,"browser_action"           : {
                              }
}
